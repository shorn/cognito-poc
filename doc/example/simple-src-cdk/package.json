{
  "name": "simple-cdk",
  "version": "0.1.0",
  "bin": {
    "simple-src-cdk": "Main.js"
  },
  "scripts": {
    "build": "tsc --noEmit",
    "watch": "tsc --watch --noEmit",
    "test": "jest",
    "zinc": "cdk --profile zinc",
    "synth": "npm run zinc synth",
    "diff": "npm run zinc diff",
    "bootstrap": "npm run zinc bootstrap",
    "deploy": "npm run zinc deploy",
    "deploy-awscdkstack": "npm run zinc deploy AwsCdkStack",
    "deploy-lambda": "npm run zinc deploy LambdaStack",
    "hotswap-lambda": "cdk --hotswap --profile zinc deploy LambdaStack",
    "destroy": "npm run zinc donot_destroy",
    "doctor": "npm run zinc doctor",
    "context": "npm run zinc context"
  },
  "devDependencies": {
    "esbuild": "0.14.29",
    "@types/jest": "27.4.1",
    "@types/node": "10.17.27",
    "jest": "27.5.1",
    "ts-jest": "27.1.4",
    "aws-cdk": "2.19.0",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.1",    
    "typescript": "4.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.19.0",
    "constructs": "10.0.102",
    "source-map-support": "0.5.21"
  }
}
