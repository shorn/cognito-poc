{
  "name": "aws-cdk",
  "version": "0.1.0",
  "bin": {
    "aws-cdk": "bin/aws-cdk.js"
  },
  "scripts": {
    "postinstall": "(cd lib/poc-lambda && npm install);",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cog-poc": "cdk --profile cognito-poc",
    "synth": "npm run cog-poc synth",
    "diff": "npm run cog-poc diff",
    "bootstrap": "npm run cog-poc bootstrap",
    "deploy": "npm run cog-poc deploy",
    "deploy-awscdkstack": "npm run cog-poc deploy AwsCdkStack",
    "deploy-lambda": "npm run cog-poc deploy LambdaStack",
    "destroy": "npm run cog-poc donot_destroy",
    "doctor": "npm run cog-poc doctor",
    "context": "npm run cog-poc context"
  },
  "devDependencies": {
    "esbuild": "0.14.29",
    "@types/jest": "27.4.1",
    "@types/node": "10.17.27",
    "jest": "27.5.1",
    "ts-jest": "27.1.4",
    "aws-cdk": "2.19.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.19.0",
    "constructs": "10.0.102",
    "source-map-support": "0.5.21"
  }
}
